{
  "name": "orbithub-backend",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc && npm run copy-static",
    "copy-static": "cp -r src/public dist/ 2>/dev/null || true",
    "start": "node dist/app.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.5.0",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "node-cron": "^3.0.2",
    "dropbox": "^10.34.0",
    "puppeteer": "^21.3.6",
    "crypto-js": "^4.1.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "serve-static": "^1.15.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/morgan": "^1.9.4",
    "@types/node-cron": "^3.0.8",
    "@types/crypto-js": "^4.1.1",
    "@types/compression": "^1.7.2",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 